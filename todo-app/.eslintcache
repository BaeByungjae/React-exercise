odo.id === id ? { ...todo, checked: !todo.checked } : todo,\n      ),\n    );\n  }, []);\n  return (\n    <TodoTemplate>\n      <TodoInsert onInsert={onInsert} />\n      <TodoList onToggle={onToggle} onRemove={onRemove} todos={todos} />\n    </TodoTemplate>\n  );\n};\n\nexport default App;\n","C:\\Users\\user\\projects\\react\\todo-app\\src\\components\\TodoTemplate.js",[],"C:\\Users\\user\\projects\\react\\todo-app\\src\\components\\TodoInsert.js",[],"C:\\Users\\user\\projects\\react\\todo-app\\src\\components\\TodoListItem.js",[],"C:\\Users\\user\\projects\\react\\todo-app\\src\\components\\TodoList.js",[],{"ruleId":"37","severity":1,"message":"38","line":7,"column":10,"nodeType":"39","messageId":"40","endLine":7,"endColumn":21},"no-unused-vars","'todoReducer' is defined but never used.","Identifier","unusedVar"]